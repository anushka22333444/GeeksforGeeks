class Solution {
    public int findPeakElement(List<Integer> a) {
        // Code here
        int maxE = Integer.MIN_VALUE;
        
        // loop to access elements
        
        for(int i = 0; i<a.size(); i++)
        {
            if(a.get(i) > maxE)
            {
                maxE = a.get(i);
            }
        }
        return maxE;
    }
}
